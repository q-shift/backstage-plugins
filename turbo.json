{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["tsc", "^build"]
    },
    "clean": {
      "cache": false
    },
    "tsc": {
      "outputs": ["../../dist-types/**"],
      "dependsOn": ["^tsc"]
    }
  }
}